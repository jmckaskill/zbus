
# vendor
build obj/vendor/c-rbtree.o: cc vendor/c-rbtree-3.1.0/src/c-rbtree.c

# dbus
build obj/dbus/auth.o: cc dbus/auth.c
build obj/dbus/decode.o: cc dbus/decode.c
build obj/dbus/encode.o: cc dbus/encode.c
build obj/dbus/match.o: cc dbus/match.c
build obj/dbus/stream.o: cc dbus/stream.c

build obj/dbus.lib: lib $
        obj/dbus/auth.o $
        obj/dbus/decode.o $
        obj/dbus/encode.o $
        obj/dbus/match.o $
        obj/dbus/stream.o $

# lib

build obj/lib/file-posix.o: cc lib/file-posix.c
build obj/lib/file-win32.o: cc lib/file-win32.c
build obj/lib/log.o: cc lib/log.c
build obj/lib/logmsg.o: cc lib/logmsg.c
build obj/lib/print.o: cc lib/print.c
build obj/lib/socket-posix.o: cc lib/socket-posix.c
build obj/lib/socket-win32.o: cc lib/socket-win32.c
build obj/lib/threads-win.o: cc lib/threads-win.c

build obj/lib.lib: lib $
        obj/lib/file-posix.o $
        obj/lib/file-win32.o $
        obj/lib/log.o $
        obj/lib/logmsg.o $
        obj/lib/print.o $
        obj/lib/socket-posix.o $
        obj/lib/socket-win32.o $
        obj/lib/threads-win.o $


# tester

build obj/tester/client.o: cc tester/client.c
build obj/tester/socket-posix.o: cc tester/socket-posix.c
build obj/tester/socket-win32.o: cc tester/socket-win32.c
build obj/tester/tester.o: cc tester/tester.c

build bin/tester${exe}: ld $
        obj/tester/client.o $
        obj/tester/socket-posix.o $
        obj/tester/socket-win32.o $
        obj/tester/tester.o $
        obj/lib.lib $
        obj/dbus.lib $


build obj/zbus/addr.o: cc zbus/addr.c
