build obj/zbus/main-win32.o: cc zbus/main-win32.c
build obj/zbus/rcu.o: cc zbus/rcu.c
build obj/zbus/rx.o: cc zbus/rx.c
build obj/zbus/sec-linux.o: cc zbus/sec-linux.c
build obj/zbus/sec-posix.o: cc zbus/sec-posix.c
build obj/zbus/sec-win32.o: cc zbus/sec-win32.c
build obj/zbus/sub.o: cc zbus/sub.c
